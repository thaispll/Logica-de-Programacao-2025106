Algoritmo "valida_idade"

Var
// valide a idade de 0 a 120 anos
  idade: inteiro

Inicio

      repita
            escreval("Digite a sua idade (0 a 120):")
            
            leia(idade)
      ate ((idade >=0) e (idade <=120))
      escreval("Idade válida: ", idade)

Fimalgoritmo